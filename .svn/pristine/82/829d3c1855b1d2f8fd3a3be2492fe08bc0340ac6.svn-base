}
.checkoutbutton {
  background-color: #1c9cea !important;
}
.qualification_details {
  border: 1px solid #e0e0e0;
  margin-top: 15px;
  padding: 10px;
}
.edit,
.delete {
  float: right;
  cursor: pointer;
}
.edit i,
.delete i {
