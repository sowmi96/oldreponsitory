}
.delete {
  margin-right: 20px;
}
.submit-info button {
  float: right;
}
.container .fa-asterisk {
  float: left;
  font-size: 8px;
  margin-top: 20px;
  margin-right: 5px;
  color: #cd1517;
}

.doc_upload {
  padding: 5px 0px 0px 40px;
  border: 0px;
  position: relative;
  outline: none;
  width: 100%;
}
.doc_upload:before {
  content: 'Upload file';
  border-radius: 3px;
  position: absolute;
  left: 0px;
  top: 0px;
  width: 115px;
  padding: 5px 8px;
  outline: none;
  cursor: pointer;
  font-size: 14px;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0.1px;
  text-align: center;
  color: #ffffff;
  background-color: #000;
}
.head .fa-asterisk {
  margin-top: 5px;
  float: left;
}
.salutation {
  padding-left: 0 !important;
}
input.ng-valid {
  border: 1px solid #e0e0e0 !important;
}
input[name='diff-abled'],
input[name='destitute'],
input[name='serviceman'] {
  width: 20px;
}
.servicemen-sec {
  margin-bottom: 20px;
}
.btn-continue {
  float: right;
  margin-top: 20px;
}
.mandCaution {
  margin-top: 30px;
}
button.buybutton.btn-continue[disabled],
select.ng-dirty.ng-touched[disabled],
input.ng-dirty.ng-touched[disabled],
.final-submit[disabled] {
  background: #efefef;
  cursor: not-allowed;
  color: lightgray;
}
.submit-information > button {
  float: right;
  margin-bottom: 20px;
}
.success-apply {
  color: greenyellow;
  margin-bottom: 20px;
}
input#number,
input[type='number'] {
  -moz-appearance: textfield;
}

input[type='number']::-webkit-inner-spin-button,
input[type='number']::-webkit-outer-spin-button {
  -webkit-appearance: none;
  margin: 0;
}
select {
  font-size: 10px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0.1px;
  text-align: left;
  color: #737373;
  width: 100%;
  padding: 8px;
  margin-bottom: 12px;
  border-radius: 4px;
  background-color: #ffffff;
  border: solid 1px #bdbdbd;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}

.form-group-ck {
  display: block;
  margin-bottom: 15px;
}

.form-group-ck input {
  padding: 0;
  height: initial;
  width: initial;
  margin-bottom: 0;
  display: none;
  cursor: pointer;
}

.form-group-ck label {
  position: relative;
  cursor: pointer;
  display: flex;
  align-items: center;
}

.form-group-ck label:before {
  content: '';
  -webkit-appearance: none;
  background-color: transparent;
  border: 2px solid #0079bf;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05),
    inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05);
  padding: 8px;
  display: inline-block;
  position: relative;
  vertical-align: middle;
  cursor: pointer;
  margin-right: 5px;
}

.form-group-ck input:checked + label:after {
  content: '';
  display: block;
  position: absolute;
  top: 2px;
  left: 7px;
  width: 6px;
  height: 12px;
  border: solid #0079bf;
  border-width: 0 2px 2px 0;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}

i.fa-check {
  margin-left: 15px;
  color: lightseagreen;
  border: 2px solid lightseagreen;
  border-radius: 50%;
  padding: 3px;
  font-size: 12px;
  display: flex;
  align-items: center;
}
