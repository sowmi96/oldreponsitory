}
.rightRs {
  position: absolute;
  margin: auto;
  display: block;
  outline: none;
  border-radius: 50%;
  top: 120%;
  bottom: 0;
  width: 31px;
  background-color: #ffffff !important;
  height: 31px;
  background-color: #ffffff;
  right: 45%;
  background-image: none;
}
.leftRs {
  position: absolute;
  margin: auto;
  border-radius: 50%;
  top: 120%;
  display: block;
  bottom: 0;
  width: 31px;
  height: 31px;
  background-color: #ffffff !important;
  -webkit-box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.2);
  box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.2);
  left: 45%;
  background-image: none;
}
input.ng-invalid.ng-touched {
  border: 1px solid red;
}
.scheme-hide {
