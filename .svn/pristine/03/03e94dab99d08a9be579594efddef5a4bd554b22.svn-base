  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background-color: #1c9cea;
  border-radius: 8px;
  color: #ffffff;
  font-size: 14px;
  font-family: "Muli-Bold";
  border: none;
}
.arrow-hide {
  -webkit-appearance: textfield !important;
