.mob__accordian .panel {
  border: none;
}
header {
  width: 100%;
}
a {
  text-decoration: none !important;
}
.co-optex-header {
  margin: 27px 50px 15px;
  align-items: center;
}

.pro__details .panel-heading {
  background-color: #ffffff !important;
  padding: 12px 0px;
}
.pro__details .panel:nth-child(1) {
  border: none;
  border-top: solid 0.5px #979797;
  border-radius: 0px;
  box-shadow: none;
}
.lastborder .panel {
  border-bottom: solid 0.5px #979797 !important;
}
.pro__details .panel-group .panel + .panel {
  margin-top: 0px;
}
.pro__details .panel-body {
  padding: 0px 0px 15px;
}

.knt__login__form input,
.knt__login__form select {
  font-size: 10px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0.1px;
  text-align: left;
  color: #737373;
  width: 100%;
  padding: 8px;
  margin-bottom: 12px;
  border-radius: 4px;
  background-color: #ffffff;
  border: solid 1px #bdbdbd;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}
.selectdiv {
  position: relative;
}
.selectdiv:after {
  content: '\f078';
  font: normal normal normal 13px/1 FontAwesome;
  color: #9a9a9a;
  right: 11px;
  top: 9px;
  position: absolute;
  pointer-events: none;
}

select::-ms-expand {
  display: none;
}

