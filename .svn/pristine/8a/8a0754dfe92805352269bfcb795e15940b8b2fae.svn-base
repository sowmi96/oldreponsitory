@media only screen and (min-width: 320px) {
  .error-info {
    position: absolute;
  }
  .manageAddressOuter {
    margin: 25px 25px;
    text-align: left;
  }
  .add-title {
    margin-top: 20px;
    font-size: 16px;
    font-weight: bold;
  }
  .disable {
    pointer-events: none;
  }
  .btn {
    width: 56px !important;
  }

  .inputsOuterMobile {
    width: 170px;
    margin: 0 auto;
    padding-top: 26px;
  }
  .addNewAddressButton {
    width: 184px;
    height: 32px;
    background-color: black;
    color: white;
    font-family: "Muli";
    font-size: 14px;
    font-weight: 800;
    text-align: center;
    padding-top: 4px;
    border-radius: 4px;
    cursor: pointer;
  }

  .manageAddressForm {
    display: none;
  }

  .manageAddressNameGroup {
    margin-top: 20px;
  }

  .manageAddressNameGroup > div {
    display: inline-block;
    margin-right: 10px;
    margin-top: 15px;
  }
  .manageAddressInputHeadings {
    font-family: "Muli";
    font-size: 14px;
    font-weight: 600;
    text-align: left;
    color: #393939;
  }
  .inputFields {
    width: 172px;
    height: 32px;
    border: 1px solid #979797;
    border-radius: 3px;
    padding: 5px;
  }
  .manageAddressMob {
    margin-top: 20px;
  }
  .manageAddressAddressGroup > div {
    display: inline-block;
    margin-right: 10px;
    margin-top: 25px;
  }
  .manageAddressAddressGroup {
    margin-top: 0px;
  }
  .manageAddresInputs {
    margin-top: 20px;
  }
  .manageAddresInputs > select {
    width: 172px;
    height: 32px;
    font-family: "Muli";
    font-size: 12px;
    font-weight: 600;
    text-align: left;
    color: #737373;
    background-color: white;
  }

  .manageAddrButtonGroup {
    margin-top: 15px;
    text-align: center;
    padding-right: 0px;
  }

  .manageAddr_button {
    width: 72px;
    border-radius: 4px;
    text-align: center;
    display: inline-block;
    height: 32px;
    background-color: black;
    padding-top: 2px;
    border: 1px solid #979797;
  }

  .manageAddr_button:hover {
    cursor: pointer;
  }

  .cancelButton {
    color: white;
    padding-top: 4px;
  }

  .saveButton {
    color: white;
    background-color: black;
  }

  .manageAddrSavedAddr {
    font-family: "Muli-Bold";
    font-size: 12px;
    color: #393939;
    margin-right: 110px;
  }
  .manageAddrSavedAddrText {
    font-family: "Muli";
    font-size: 12px;
    font-weight: 600;
    color: #737373;
    margin-top: 17px;
  }
  .maangeAddrSavedIcon {
    color: white;
    background-color: black;
    font-family: "Muli-Bold";
    font-size: 12px;
    border-radius: 4px;
    padding: 0px 3px;
  }
  .manageAddrPopupIcon {
    background-image: url("/assets/my-profileImages/popupIcon.png");
    width: 3px;
    height: 16px;
    float: right;
    cursor: pointer;
  }
  .manageAddrEditGroup {
    width: 72px;
    height: 22px;
    border-radius: 4px;
    background-color: #ffffff;
    -webkit-box-shadow: 3px 3px 12px 0 rgba(0, 0, 0, 0.2);
    box-shadow: 3px 3px 12px 0 rgba(0, 0, 0, 0.2);
    float: right;
    cursor: pointer;
  }
  .EditGroup {
    padding: 2px 0px 2px 7px;
    font-family: "Muli";
    font-size: 12px;
    font-weight: 600;
    color: #393939;
  }
  .editButton {
    border-bottom: solid 1px #e0e0e0;
  }
}

@media only screen and (min-width: 768px) {
  .inputsOuterMobile {
    width: 100%;
    padding-top: 26px;
  }
  .add-title {
    font-size: 20px;
  }
}
.disableAddnewAddr {
  pointer-events: none;
  background-color: grey;
}
