.container-radio {
  display: block;
  position: relative;
  padding-left: 35px;
  margin-bottom: 12px;
  cursor: pointer;
  font-size: 22px;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.address-data {
  word-break: break-word;
}
.error-info {
  font-size: 12px;
  font-weight: 200;
  color: red;
  font-family: Muli-SemiBold;
}

.new-address {
  height: 55px;
  border: 1px solid;
  font-size: 20px;
  margin-bottom: 30px;
  cursor: pointer;
}
.addressForm select {
  height: 33px;
  background: white;
}
.container-radio input {
  position: absolute;
  opacity: 0;
  cursor: pointer;
}

.checkmark {
  position: absolute;
  top: 0;
  left: 0;
  height: 25px;
  width: 25px;
  background-color: #eee;
  border-radius: 50%;
}

.container-radio:hover input ~ .checkmark {
  background-color: #ccc;
}

.container-radio input:checked ~ .checkmark {
  background-color: #2196f3;
}

.checkmark:after {
  content: "";
  position: absolute;
  display: none;
}

.container-radio input:checked ~ .checkmark:after {
  display: block;
}

.container-radio .checkmark:after {
  top: 9px;
  left: 9px;
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background: white;
}

.edit {
  font-size: 14px;
  font-weight: 500;
  color: #2874f0;
  border: 0;
  background: none;
  cursor: pointer;
  outline: none;
  padding: 8px !important;
