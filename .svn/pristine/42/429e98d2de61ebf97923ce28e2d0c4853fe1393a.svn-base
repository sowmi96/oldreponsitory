/* mobile wizard css start */
.nav-pills {
  width: 299px;
  position: relative;
  left: -18px;
}

.nav-pills.nav-wizard > li {
  position: relative;
  overflow: visible;
  border-right: 6px solid #fff;
  border-left: 14px solid #fff;
  background-color: #f2f2f2;
  color: #737373;
}
.nav-pills.nav-wizard > li:first-child {
  border-left: 0;
}
.nav-pills.nav-wizard > li:first-child a {
  border-radius: 5px 0 0 5px;
}
.nav-pills.nav-wizard > li:last-child {
  border-right: 0;
}
.nav-pills.nav-wizard > li:last-child a {
  border-radius: 0 5px 5px 0;
}
.nav-pills.nav-wizard > li a {
  border-radius: 0;
  width: 85px;
  font-family: "Muli-SemiBold";
  font-size: 8px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0px;
  text-align: left;
  padding: 0;
  height: 42px;
  display: flex;
  align-items: center;
  padding-left: 17px;
}
.nav-pills.nav-wizard > li .nav-arrow {
  position: absolute;
  top: 0px;
  right: -20px;
  width: 0px;
  height: 0px;
  border-style: solid;
  border-width: 21px 0 21px 20px;
  border-color: transparent transparent transparent #f2f2f2;
  z-index: 150;
}
.nav-pills.nav-wizard > li .nav-wedge {
  position: absolute;
  top: 0px;
  left: -20px;
  width: 0px;
  height: 0px;
  border-style: solid;
  border-width: 21px 0 21px 20px;
  border-color: #f2f2f2 #f2f2f2 #f2f2f2 transparent;
  z-index: 150;
}

.nav-pills.nav-wizard > li.active1 .nav-arrow {
  border-color: transparent transparent transparent #1c9cea;
}
.nav-pills.nav-wizard > li.active1 .nav-wedge {
  border-color: #1c9cea #1c9cea #1c9cea transparent;
}
.nav-pills.nav-wizard > li.active1 a {
  color: #fff;
}
/* mobile wizard css end */

