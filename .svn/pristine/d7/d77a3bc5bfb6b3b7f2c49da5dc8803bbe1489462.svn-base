@media only screen and (min-width: 320px) and (max-width: 766px) {
    .modal-dialog {
        width: 264px !important;
    }
}
.caution {
    text-align: left;
    color: #c41b1f;
    margin-top: 10px;
    font-weight: bold;
}
.header-log{
    float: left;
    font-weight: 600;
    font-size: 31px;
    font-family: Muli;
}
.credential-label {
    float: left;
    font-size: 14px;
    font-weight: 200;
    font-family: Muli;
}
.modal-dialog {
    width: 446px;
    margin: 30px auto;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%) !important;
}
.modal-dialog .btn-primary {
    width: 100%;
    background-color: #0086EB;
    border-color: #0086EB;
    margin-top: 15px;
}
.forgot-password {
    color: #1c9cea;
    float: left;
}
a {
    cursor: pointer;   
    font-family: Muli;     
}
.sign-up {
    text-align: left;
    margin-top: 15px;
    font-family: Muli;
}
.sign-up span:nth-child(2) {
    color: #1c9cea;
}
.btn-primary {
    font-family: Muli;
}
button.close {
    color: #cd1517;
}
.modal-body {
    padding: 20px;
}
