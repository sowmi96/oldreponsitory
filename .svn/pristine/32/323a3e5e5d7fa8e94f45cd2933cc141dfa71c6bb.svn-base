div,
span,
ul,
li {
  font-family: Muli;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0.1px;
}
.no-padding {
  padding: 0 !important;
}

.sort-opt select {
  background: transparent;
  border-radius: 3px;
  width: 95%;
  padding: 5px;
  border: 1px solid #e0e0e0;
}

.career-header {
  border-bottom: 1px solid #e0e0e0;
}
.job-details-section {
  border-top: 1px solid #e0e0e0;
}
.app-status span {
  color: #fff;
  border-radius: 5px;
  float: right;
}

.app-status span.statusNone {
  background: #3c3c3c;
}

.app-status {
  padding: 10px;
}
.desc .desc-details p {
  width: 525px;
}
.job-portal {
  width: 100%;
}
.container .tab-cont {
  display: none;
}
/* .job-portal {
    display: none;
}
.job-portal.active {
    display: block;
} */
.job-opening {
  padding: 10px;
  -webkit-box-shadow: 2px 2px 6px 0 rgba(0, 0, 0, 0.2);
  box-shadow: 2px 2px 6px 0 rgba(0, 0, 0, 0.2);
  border-left: 3px solid #bdbdbd;
  margin-bottom: 20px;
}
.job-opening.active {
  border-left: 3px solid #1c9cea;
}
.job-main-cont {
  cursor: pointer;
}
.role {
  float: left;
}

.job-code {
  float: left;
  clear: both;
}

.status-sec {
  position: relative;
}

.stepper .nav-tabs {
  position: relative;
}

.stepper .nav-tabs > li {
  width: 25%;
  position: relative;
}
.desc {
  padding-bottom: 15px !important;
}
.stepper .nav-tabs > li:after {
  content: '';
  position: absolute;
  background: #f1f1f1;
  display: block;
  width: 100%;
  height: 5px;
  top: 30px;
  left: 50%;
  z-index: 1;
}

.nav-tabs {
  border: 0 !important;
}

/* .stepper .nav-tabs > li.completed::after {
    background: #34bc9b;
  } */

.stepper .nav-tabs > li:last-child::after {
  background: transparent;
}

/* .stepper .nav-tabs > li.active:last-child .round-tab {
    background: #34bc9b;
  } */

.stepper .nav-tabs > li.active:last-child .round-tab::after {
  content: 'âœ”';
  color: #fff;
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 auto;
  top: 0;
  display: block;
}

.stepper .nav-tabs > li:last-child::after {
  width: 0;
}

.stepper .nav-tabs [data-toggle='tab'] {
  width: 25px;
  height: 25px;
  margin: 20px auto;
  border-radius: 100%;
  border: none;
  padding: 0;
  color: #f1f1f1;
}

.stepper .nav-tabs [data-toggle='tab']:hover {
  background: transparent;
  border: none;
}

.stepper .nav-tabs > .active > [data-toggle='tab'],
.stepper .nav-tabs > .active > [data-toggle='tab']:hover,
.stepper .nav-tabs > .active > [data-toggle='tab']:focus {
  color: #34bc9b;
  cursor: default;
  border: none;
}

.stepper .tab-pane {
  position: relative;
  padding-top: 50px;
}

.stepper .round-tab {
  width: 25px;
  height: 25px;
  line-height: 22px;
  display: inline-block;
  border-radius: 25px;
  background: #fff;
  border: 2px solid #e0e0e0;
  color: #34bc9b;
  z-index: 2;
  position: absolute;
  left: 0;
  text-align: center;
  font-size: 14px;
}

.stepper .round-tab.activeStatus {
  background-image: url(/assets/tick.svg);
  background-size: 22px 22px;
  background-repeat: no-repeat;
}

/* .stepper .completed .round-tab {
    background: #34bc9b;
  } */

.stepper .completed .round-tab::after {
  content: '';
  color: #fff;
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 auto;
  top: 0;
  display: block;
}

/* .stepper .active .round-tab {
    background: #fff;
    border: 2px solid #34bc9b;
  }
  .stepper .active .round-tab:hover {
    background: #fff;
    border: 2px solid #34bc9b;
  } */

.stepper .active .round-tab::after {
  display: none;
}

.stepper .disabled .round-tab {
  background: #fff;
  color: #f1f1f1;
  border-color: #f1f1f1;
}

.stepper .disabled .round-tab:hover {
  color: #4dd3b6;
  border: 2px solid #a6dfd3;
}

.stepper .disabled .round-tab::after {
  display: none;
}

.desc-header.header {
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0.1px;
  text-align: left;
  color: #393939;
  margin-top: 15px;
  padding-left: 15px;
  padding-right: 15px;
}

.desc-details.details {
  font-family: Muli;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.6;
  letter-spacing: 0.1px;
  text-align: left;
  color: #737373;
  margin-top: 15px;
  padding-left: 15px;
  padding-right: 15px;
}

.upload-btn {
  border: 0px;
  position: relative;
  outline: none !important;
  border-radius: 5px;
}

input.upload-btn::before {
  content: 'Attach Documents';
  position: absolute;
  left: 0px;
  top: 0px;
  outline: none;
  cursor: pointer;
  font-size: 12px;
  font-weight: bold;
  border-radius: 8px;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0.1px;
  text-align: center;
  color: #ffffff;
  font-family: 'Muli-Bold';
  background-color: #000;
  border-radius: 5px;
}
.primary-font {
  font-weight: 800;
  text-align: left;
  color: #393939;
}
.secondary-font {
  font-weight: 600;
  text-align: left;
  color: #737373;
}
.applied {
  border-bottom: 1px solid #e0e0e0;
  margin-top: 15px;
  margin-top: 15px;
  border-top: 1px solid #e0e0e0;
  padding-top: 15px !important;
}
.progress-text {
  color: #737373;
  font-weight: 600;
}
.msg:first-child {
  color: #393939;
  font-weight: bold;
}
.msg:nth-child(2) {
  color: #c41b1f;
  margin-bottom: 15px;
  font-weight: 600;
  margin-top: 5px;
}
.status-text.red {
  color: #c41b1f;
}
li.tab-item.active span {
  border-bottom: 2px solid #1c9cea;
}
li.tab-item {
  cursor: pointer;
}
.exam-details-section.written {
  border-top: 1px solid #e0e0e0;
}
